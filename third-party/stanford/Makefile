STANFORD_SYMLINK=stanford
STANFORD=stanford-corenlp-full-2014-10-31
STANFORD_ARCHIVE=$(STANFORD).zip
STANFORD_URL=http://nlp.stanford.edu/software/$(STANFORD_ARCHIVE)

default: $(STANFORD_ARCHIVE)
	unzip $(STANFORD_ARCHIVE)
	ln -s $(STANFORD) $(STANFORD_SYMLINK)

$(STANFORD_ARCHIVE):
	wget $(STANFORD_URL)

clean:
	rm -rf *~
	rm -rf $(STANFORD) $(STANFORD_ARCHIVE)
	rm -f $(STANFORD_SYMLINK)
