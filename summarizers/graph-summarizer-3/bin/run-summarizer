#!/bin/bash -x

SUMMARIZER_MAKEFILE_LOCATION=$1
SUMMARIZER_MAKEFILE_TARGET=$2
SUMMARIZER_MAKEFILE_BASE_DIRECTORY=$3
FOLD_ID=$4
CATEGORY_BASE_NAME=$5

make -f ${SUMMARIZER_MAKEFILE_LOCATION} -C ${SUMMARIZER_MAKEFILE_BASE_DIRECTORY} ${SUMMARIZER_MAKEFILE_TARGET} FOLD_ID=${FOLD_ID} CATEGORY=${CATEGORY_BASE_NAME} 1>&2

cat ${SUMMARIZER_MAKEFILE_BASE_DIRECTORY}/${CATEGORY_BASE_NAME}.models/graph-summarizer-3/${FOLD_ID}/test.out | while read SUMMARIZER_OUTPUT_LINE; do
    echo -e "${SUMMARIZER_OUTPUT_LINE}\tgraph3";
done;
